{
  "name": "sqlrsync-docs",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "scripts": {
    "build": "pnpm build:help && pnpm build:blog && pnpm build:blog-sitemap",
    "build:help": "rm -rf ../webapp/app/pages/help; eleventy --input=help/. --output=../webapp/app/pages/help",
    "build:blog": "rm -rf ../webapp/app/pages/blog; eleventy --input=blog/. --output=../webapp/public/blog",
    "build:blog-sitemap": "rm -rf ../webapp/public/blog/sitemap.xml; eleventy --input=blog/. --output=../webapp/public/blog",
    "dev": "rm -rf ../webapp/app/pages/help; eleventy --watch --input=help/. --incremental --output=../webapp/app/pages/help",
    "dev:blog": "rm -rf ../webapp/app/pages/blog; eleventy --watch --input=blog/. --incremental --output=../webapp/public/blog",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@mdx-js/mdx": "^3.1.1",
    "debug": "^4.4.1",
    "liquidjs": "^10.21.1",
    "nunjucks": "^3.2.4"
  }
}